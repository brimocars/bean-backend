{
    "extends": "airbnb/base",
    "rules": {
      "no-underscore-dangle": "off",
      "no-plusplus": "off",
      "import/extensions": "off",
      "no-param-reassign": "off",
      "max-len": ["error", { "code": 120 }],
      "no-console": "off",
      "no-use-before-define": "off",
      "no-restricted-syntax": "off",
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "jest": true
    }
  }